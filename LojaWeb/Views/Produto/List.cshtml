@model ProdutosListViewModel



<h6>@Model.CategoriaAtual</h6>
<div>
    <h2>@Model.CategoriaAtual</h2>                                                                              
</div>
<hr />
<h3>@ViewBag.Total @ViewBag.TotalProdutos</h3>
<hr />



<div class="container">
    <div class="row">
        @foreach (var produto in Model.Produtos)
        {
            <partial name="_ProdutosResumo" for="@produto" />
        }
    </div>
</div>
